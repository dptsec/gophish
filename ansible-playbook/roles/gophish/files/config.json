{
  "admin_server": {
    "listen_url": "127.0.0.1:3333",
    "use_tls": true,
    "cert_path": "/etc/ssl/crt/gophish.crt",
    "key_path": "/etc/ssl/private/gophish.pem",
    "allowed_internal_hosts": [],
    "trusted_origins": []
  },
  "phish_server": {
    "listen_url": "127.0.0.1:8080",
    "use_tls": true,
    "cert_path": "/etc/ssl/crt/gophish.crt",
    "key_path": "/etc/ssl/private/gophish.pem",
    "ip_blacklist": [],
